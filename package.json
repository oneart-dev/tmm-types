{
  "name": "tmm-types",
  "version": "0.1.1",
  "type": "module",
  "types": "./index.d.ts",
  "description": "TMM auto generated types from SWAG docs",
  "scripts": {
    "generate": "swagger-typescript-api --clean-output --axios --modular -p ./src/swagger.json -o ./types",
