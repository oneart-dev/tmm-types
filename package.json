    "build": "yarn run generate && yarn run export"
  },
  "repository": "git@github.com:oneart-dev/tmm-types.git",
  "author": "Nick S <oart4k@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0",
    "yarn": ">= 1.22.0"
  },
  "devDependencies": {
    "swagger-typescript-api": "^13.0.3"
  }
}
